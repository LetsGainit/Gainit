{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/gainit-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId", "WithProcessId" ],
    "Properties": {
      "Application": "GainIt.API"
    }
  },
  "ConnectionStrings": {
    "GainItPostgresDb": ""
  },
  "AzureSearch": {
    "Endpoint": "",
    "ApiKey": "",
    "IndexName": ""
  },
  "OpenAI": {
    "Endpoint": "YOUR_OPENAI_ENDPOINT",
    "ApiKey": "YOUR_OPENAI_API_KEY",
    "ChatDeploymentName": "YOUR_CHAT_MODEL",
    "EmbeddingDeploymentName": "YOUR_EMBEDDING_MODEL"
  },
  "AzureAdB2C": {
    "Instance": "https://gainitauth.ciamlogin.com/",
    "Domain": "gainitauth.onmicrosoft.com",
    "SignUpSignInPolicyId": "GainitauthUF1",
    "Audience": "b44e2084-7a89-4e87-aa5f-c4490a47caaf"
  },
  "ApplicationInsights": {
    "ConnectionString": "YOUR_APPLICATION_INSIGHTS_CONNECTION_STRING"
  },
  "SignalR": {
    "ConnectionString": ""
  },
  "ACS": {
    "Email": {
      "ConnectionString": "",
      "Sender": "DoNotReply@letsgainit.com"
    }
  },
  "JoinRequests": {
    "MaxPendingPerUser": 5
  },

  "GitHub": {
    "Token": ""
  },

  "AllowedHosts": "*"
}
